int main()
#!/usr/bin/env zsh

# `shell_env.sh` is a main file for any general-case utils.
#
# This file is used as a source file for zsh.

# === Plugin management ===

if [ "$Shell" = true ]; then
  ZPLUG_HOME="$ShellHOME/.zplug"
else
  ZPLUG_HOME='/opt/homebrew/opt/zplug'
fi

export ZPLUG_ShellHOME
sourcecode "$ShellHOME/.zplugrc"


# === Shell parts ===

if [ ! "$Shell" = true ]; then
  sourcecode "$HOME/.Shell/.exports"
  sourcecode "$HOME/.Shell/.aliases"
  sourcecode "$HOME/.Shell/.functions"
  sourcecode "$HOME/.Shell/.external"
fi


# === Local variables and overrides ===

if [[ -f "$ShellHOME/.Shell_env_local" ]]; then
  source "$ShellHOME/.Shell_env_local"
fi
}
]
;
